{
    "name": "nguyenloc879/nguon",
    "description": "nguonc crawler",
    "type": "library",
    "require": {
        "hacoidev/nguonc-core": "^1.0.0",
        "intervention/image": "^2.7"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nguonc\\Crawler\\nguoncCrawler\\": "src/"
        }
    },
    "authors": [
        {
            "name": "hacoidev",
            "email": "hacoi.dev@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Nguonc\\Crawler\\nguoncCrawler\\nguoncCrawlerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "autoload-dev": {
        "psr-4": {
            "Nguonc\\Crawler\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }
}
